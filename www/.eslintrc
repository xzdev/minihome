{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "settings": {
        "import/resolver": "webpack"
    },
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]        
    },
    "globals": {
        "document": true,
        "window": true
    },
    "env": {
        "browser": true,
        "node": true
    }
}